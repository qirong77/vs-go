{
  "name": "vsgo",
  "version": "1.0.0",
  "description": "An Electron application with React and TypeScript",
  "main": "./out/main/index.mjs",
  "author": "qirong77.github.com",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "preview": "electron-vite preview",
    "dev": "electron-vite dev",
    "dev:monaco": "vite --config ./monaco-markdown-dev/vite.config.ts",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "prebuild:mac": "node ./prebuild.mjs",
    "build:mac": "electron-vite build && electron-builder --mac",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "@types/cheerio": "^0.22.35",
    "@udecode/plate-basic-elements": "^49.0.0",
    "@udecode/plate-basic-marks": "^48.0.0",
    "@udecode/plate-common": "^41.0.13",
    "@udecode/plate-heading": "^48.0.0",
    "@udecode/plate-list": "^48.0.0",
    "@udecode/plate-paragraph": "^36.0.0",
    "ansi-to-html": "^0.7.2",
    "antd": "^6.1.0",
    "autoprefixer": "^10.4.19",
    "cheerio": "^1.1.2",
    "electron-context-menu": "^4.1.1",
    "electron-store": "^10.1.0",
    "electron-updater": "^6.3.9",
    "monaco-editor": "^0.54.0",
    "p-map": "^5.5.0",
    "pinyin": "^4.0.0-alpha.0",
    "slate": "^0.118.1",
    "slate-history": "^0.113.1",
    "slate-react": "^0.117.4",
    "tailwindcss": "^3.4.3"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@electron-toolkit/eslint-config-ts": "^3.1.0",
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@types/node": "^22.18.6",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "electron": "^38.1.2",
    "electron-builder": "^25.1.8",
    "electron-vite": "^4.0.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2",
    "vite": "^7.1.6"
  }
}
